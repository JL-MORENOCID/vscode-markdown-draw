{
	"name": "vscode-markdown-draw",
	"displayName": "Draw Note",
	"description": "Draw svg quick-responsively with a mouse or pen, designed for note-taking, formulas drafting. [Prototype stage now!]",
	"version": "0.1.1",
	"publisher": "zhaouv",
	"license": "Apache-2.0",
	"repository": {
		"url": "https://github.com/zhaouv/vscode-markdown-draw.git"
	},
	"bugs": {
		"url": "https://github.com/zhaouv/vscode-markdown-draw/issues"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"markdown",
		"note",
		"draw",
		"svg"
	],
	"activationEvents": [
		"onCommand:markdownDraw.editCurrentLineAsSVG",
		"onLanguage:markdown"
	],
	"main": "./src/extension",
	"contributes": {
		"commands": [
			{
				"command": "markdownDraw.editCurrentLineAsSVG",
				"title": "Edit/Create current line as SVG",
				"category": "Markdown Draw"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "markdownDraw.editCurrentLineAsSVG",
					"when": "editorFocus",
					"group": "7_modification" 
				}
			]
		}
	},
	"scripts": {},
	"dependencies": {},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/node": "^12.12.0"
	}
}
